<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <!--ID: Users-->
    <add key="NeylionID" value="ENTER ID HERE"/>
    <!--ID: Roles-->
    <add key="Admin" value="ENTER ID HERE"/>
    <add key="Moderator" value="ENTER ID HERE"/>
    <!--Tokens-->
    <add key="BotToken" value="ENTER BOT TOKEN HERE"/>
    <!--Database-->
    <add key="ConnectionString" value="ENTER CONNECTION STRING FOR DATABASE HERE" />
  </appSettings>
  <system.data>
    <DbProviderFactories>
      <remove invariant="MySql.Data.MySqlClient" />
      <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.9.9.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
    </DbProviderFactories>
  </system.data>
</configuration>